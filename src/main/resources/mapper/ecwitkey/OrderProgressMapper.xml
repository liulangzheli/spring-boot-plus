<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="io.geekidea.springbootplus.ecwitkey.mapper.OrderProgressMapper">

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, start_time, end_time, intro, source_id, order_id, remark
    </sql>

    <select id="getOrderProgressById" resultType="io.geekidea.springbootplus.ecwitkey.vo.OrderProgressQueryVo">
        select
        <include refid="Base_Column_List"/>
        from order_progress where id = #{id}
    </select>

    <select id="getOrderProgressPageList" resultType="io.geekidea.springbootplus.ecwitkey.vo.OrderProgressQueryVo">
        select
        <include refid="Base_Column_List"/>
        from order_progress
    </select>

</mapper>
